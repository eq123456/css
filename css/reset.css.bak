body,div,dl,dt,dd,ul,li,h1,h2,h3,h4,h5,h6,input,form,a,p,textarea{
	marjgin:0;
	padding:0;
	font-family:"Helevetica Neue",Helevetica,Arial,sans-serif;
}
ol,ul,li{
	list.style:none;
	display:block;
}
a{
	text-decoration:none;
	display:block;
}
img{
	border:none;
	display:block;
	}
.clearfloat{
	zoom:1;
}
.clearfloat:after{
	display:block;
	clear:both;
	content:'';
	visibility:hidden;
	height:0;
	}
